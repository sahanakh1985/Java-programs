
public class ArithmeticOperators {
	public static void main(String[] args) {
		System.out.println(4+98);//add
		System.out.println(73-42);//subtract
		System.out.println(12*5);//multiply
		System.out.println(60/6);//divide
		System.out.println(40/80);//divide
		System.out.println(10%5);//modulus
		System.out.println(76%10);//modulus
		System.out.println(832%987);//modulus
	}

}
